{
    "name": "ruliname",
    "version": "0.1.1",
    "description": "A Chromium extension that automatically renames tabs based on predefined rules",
    "type": "module",
    "scripts": {
        "watch": "webpack --mode=production --watch --config webpack.config.js",
        "build": "webpack --mode=production --config webpack.config.js",
        "lint": "eslint .",
        "pack": "node pack.js",
        "repack": "npm run build && npm run pack",
        "test": "mocha tests/**/*.test.js"
    },
    "dependencies": {
        "sweetalert2": "^11.11.0"
    },
    "devDependencies": {
        "@babel/core": "^7.24.6",
        "@babel/preset-env": "^7.24.6",
        "@eslint/js": "^9.3.0",
        "adm-zip": "^0.5.12",
        "babel-loader": "^9.1.3",
        "chai": "^5.1.1",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^7.1.2",
        "eslint": "^9.3.0",
        "globals": "^15.3.0",
        "jsdom": "^24.1.0",
        "mini-css-extract-plugin": "^2.9.0",
        "mocha": "^10.4.0",
        "sinon": "^18.0.0",
        "style-loader": "^4.0.0",
        "urlpattern-polyfill": "^10.0.0",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4"
    },
    "author": "Ohad Vano",
    "license": "MIT"
